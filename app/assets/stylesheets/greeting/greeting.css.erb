html {
  /* background-color: rgba(242, 243, 244,1); */
  background-color: #f2f2f2;
}
.welcome {
  display: none;
  font-size: 24px;
  font-family: "Interstate","Lucida Grande","Lucida Sans Unicode","Lucida Sans",Garuda,Verdana,Tahoma,sans-serif;
  color: white;
  text-align: center;
  margin-top: 40px;

}

.header-nav {
  position: absolute;
  height:60px;
  right: 1%;
  z-index: 1;

}

.test-title {
  display: block;
  position: relative;
}

.header-container {
  position: relative;
  width: 1240px;
  height: 450px;
  overflow: hidden;
  margin-left: auto;
  margin-right: auto;
}

.header1 {
  position: relative;
  display: inline-block;
  width: 1240px;
  height: 450px;
  background-image: url(https://a-v2.sndcdn.com/assets/images/home/hp_image@2x-ae2160d.jpg);
  background-size: 100%;
  vertical-align: top;
}

.header2 {
  position: relative;
  display: inline-block;
  width: 1240px;
  height: 450px;
  background-image: url(https://a-v2.sndcdn.com/assets/images/home/hp_image_creator_slide@2x-8925b63.jpg);
  background-size: 100%;
}

.carousel {
  /* display: inline; */
  height: 450px;
  width: 300%;
  background-color: blue;
  transition: transform 0.6s ease-in-out 0s;
}

.session-nav-buttons {
  color: white;
  font-size: 14px;
  float: right;
  font-family: "Interstate","Lucida Grande","Lucida Sans Unicode","Lucida Sans",Garuda,Verdana,Tahoma,sans-serif;
  text-decoration: none;
  padding-right: 15px;
  padding-left: 15px;
  padding-top: 6px;
  padding-bottom:6px;
  margin-top:20px;
  border-radius: 3px;
}

.all-track-index {
  background-color: white;
  width: 1000px;
  margin: 0 auto;
}

.greeting-title {
  margin-top: 85px;
  text-align: center;
  font-size: 35px;
  color: white;
}

.greeting-text {
  text-align: center;
  color: white;
  margin-top: 30px;
}

.nav-dots-container {
  position: absolute;
  display: inline-block;
  width: 100%;
  z-index: 1;
  bottom: 40px;

}

.nav-dots {
  display: flex;
  height: 10px;
  width: 45px;
  margin: auto;

  justify-content: space-between;
}

.dot-button {
  height: 10px;
  width: 10px;
  border: 1px solid white;
  border-radius: 50%;
  cursor: pointer;
}

#signup {
  margin-left: 10px;
  margin-right:30px;
  background-color: #f50;
  border: 1px solid #f50;
}

#signin, #logout {
  border: 1px solid #ccc;
}

#signin:hover, #logout:hover {
  border: 1px solid white;

}


#logout {
  float: left;
  margin-left: 30px;
  margin-top: 0px;
}

#orange-banner {
  height: 4px;
  background-color: #f50;
}
