html {
  /* background-color: rgba(242, 243, 244,1); */
  background-color: #f2f2f2;
}
.welcome {
  display: none;
  font-size: 24px;
  font-family: "Interstate","Lucida Grande","Lucida Sans Unicode","Lucida Sans",Garuda,Verdana,Tahoma,sans-serif;
  color: white;
  text-align: center;
  margin-top: 40px;

}

.header-nav {
  height:60px;
}
.header {
  width: 1240px;

  /* height: 755px; */
  height: 450px;
  margin-left: auto;
  margin-right: auto;
  background-image: url(<%= asset_path '30_OWSLA_live.jpg' %>);
  background-image: url(https://a-v2.sndcdn.com/assets/images/home/hp_image@2x-ae2160d.jpg);

  background-size: 100%;

}

.session-nav-buttons {
  color: white;
  font-size: 14px;
  float: right;
  font-family: "Interstate","Lucida Grande","Lucida Sans Unicode","Lucida Sans",Garuda,Verdana,Tahoma,sans-serif;
  text-decoration: none;
  padding-right: 15px;
  padding-left: 15px;
  padding-top: 6px;
  padding-bottom:6px;
  margin-top:20px;
  border-radius: 3px;

}


#signup {
  margin-left: 10px;
  margin-right:30px;
  background-color: #f50;
  border: 1px solid #f50;
}

#signin, #logout {
  border: 1px solid #ccc;
}

#signin:hover, #logout:hover {
  border: 1px solid white;

}


#logout {
  float: left;
  margin-left: 30px;
  margin-top: 0px;
}

#orange-banner {
  height: 4px;
  background-color: #f50;
}

.all-track-index {
  background-color: white;
  width: 1000px;
  margin: 0 auto;
}

.greeting-title {
  margin-top: 85px;
  text-align: center;
  font-size: 35px;
  color: white;


}

.greeting-text {
  /* width: 400px; */
  text-align: center;
  color: white;
  margin-top: 30px;
}
